//apply bind and call

function getName(user){
    return `${user}, ${this.fname} ${this.lname}`;
}

let p = {
    fname:'Manish',
    lname:' kumar'
}

//call
console.log(getName.call(p, 'hey!'))

console.log(getName.apply(p, ['hi']));


